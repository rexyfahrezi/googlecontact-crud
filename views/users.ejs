<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='css/style.css' />
  </head>
  <body>
    <h3>Halo <%= namaUser %></h3>
    <p>Nama Anda : <%= namaLengkapUser %> </p>
    <p>Email Anda : <%= emailUser %> </p>
    <br>
    <br>
    <h3>Daftar Kontak :</h3>
    <% if (namaKontak.length < 1) { %>
      <h4>Tidak ada kontak terdaftar</h4> 
    <% } %> 
    <ul>
      <% namaKontak.forEach(kontak => { %>
      <br>
      <li>
        <p>Nama : <%= JSON.stringify(kontak.kontaknama) %></p>
        <p>Email : <%= JSON.stringify(kontak.kontakemail) %></p>
        <p>No HP : <%= JSON.stringify(kontak.nomorhp) %></p>
      </li>
      <% }); %>
    </ul>
    
      
    
    
  </body>
</html>